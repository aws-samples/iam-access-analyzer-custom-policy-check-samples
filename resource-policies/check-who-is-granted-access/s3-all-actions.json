{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowPrincipalBlock",
      "Effect": "Allow",
      "Principal": {
        "AWS": [
          "arn:aws:iam::111111111111:role/MyPrivilegedRole",
          "arn:aws:iam::111111111111:role/MyPrivilegedRole2"
        ]
      },
      "Action": "s3:*",
      "Resource": "*"
    },
    {
      "Sid": "AllowPrincipalArnKey",
      "Effect": "Allow",
      "Principal": {
        "AWS": "*"
      },
      "Action": "s3:*",
      "Resource": "*",
      "Condition": {
        "ArnLike": {
          "aws:PrincipalArn": [
            "arn:aws:iam::111111111111:role/MyPrivilegedRole",
            "arn:aws:iam::111111111111:role/MyPrivilegedRole2"
          ]
        }
      }
    }
  ]
}