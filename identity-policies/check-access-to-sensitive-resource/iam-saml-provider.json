{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": "*",
            "Resource": "*"
        },
        {
            "Effect": "Deny",
            "Action": [
                "iam:CreateSAMLProvider",
                "iam:DeleteSAMLProvider",
                "iam:GetSAMLProvider",
                "iam:ListSAMLProviderTags",
                "iam:TagSAMLProvider",
                "iam:UntagSAMLProvider",
                "iam:UpdateSAMLProvider"
            ],
            "Resource": "arn:aws:iam::*:saml-provider/MySensitiveSamlProvider"
        }
    ]
}